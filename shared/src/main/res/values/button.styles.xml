<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Clean.Theme.Selectable">
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
    </style>

    <style name="Clean.Theme.RadioButton" parent="Widget.MaterialComponents.CompoundButton.RadioButton">
        <item name="buttonTint">@color/radio_button_tint</item>
        <item name="android:button">@drawable/billing_radio_button</item>
        <item name="materialThemeOverlay">@style/Clean.ThemeOverlay.RadioButton</item>
    </style>

    <style name="Clean.ThemeOverlay.RadioButton" parent="">
        <item name="colorOnSurface">@android:color/white</item>
        <item name="colorSecondary">@color/colorPrimary</item>
    </style>

</resources>